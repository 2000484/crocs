{
	"name": "schoology",
	"version": "1.0.0",
	"description": "Schoology - A secure private web proxy for accessing any website anonymously.",
	"type": "module",
	"engines": {
		"npm": ">=7.0.0",
		"node": ">=16.0.0"
	},
	"scripts": {
		"start": "node src/index.js",
		"build": "echo 'Building static site for GitHub Pages' && cp -r public/* . 2>/dev/null; true",
		"format": "prettier --write .",
		"lint": "eslint ./src/",
		"lint:fix": "eslint ./src/ --fix"
	},
	"keywords": [
		"proxy",
		"privacy",
		"web-browser",
		"bypass"
	],
	"author": "Schoology Contributors",
	"license": "GNU AFFERO",
	"dependencies": {
		"fastify": "^5.4.0",
		"@fastify/static": "^8.2.0",
		"@mercuryworkshop/bare-mux": "^2.1.7",
		"@mercuryworkshop/libcurl-transport": "^1.5.2",
		"@mercuryworkshop/scramjet": "https://github.com/MercuryWorkshop/scramjet/releases/download/latest/mercuryworkshop-scramjet-2.0.0-alpha.tgz",
		"@mercuryworkshop/wisp-js": "^0.4.1",
		"ws": "^8.18.3"
	},
	"devDependencies": {
		"eslint": "^9.13.0",
		"prettier": "^3.3.3"
	},
	"packageManager": "pnpm@10.18.3"
}
